{
  "name": "svger-biz-utils",
  "version": "0.0.1",
  "description": "业务公共 utils",
  "main": "lib/index.js",
  "module": "es/index.js",
  "license": "ISC",
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "test": "webpack --config webpack.config.js && mocha ./test/build/*.js",
    "lint": "node_modules/.bin/tools run lint",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir ./lib",
    "build:es": "cross-env BABEL_ENV=es babel ./src --out-dir ./es",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -o dist/index.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -o dist/index.min.js",
    "build": "npm run clean && npm run build:commonjs && npm run build:es",
    "clean": "rm -rf ./dist ./es ./lib"
  },
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-eslint": "~8.0.2",
    "babel-loader": "~7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-polyfill": "~6.26.0",
    "babel-preset-env": "~1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "~6.26.0",
    "cefc-tools": "^0.0.15",
    "chai": "~4.1.2",
    "cross-env": "^5.1.3",
    "eslint": "^4.13.1",
    "eslint-config-airbnb-base": "~12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "mocha": "~4.0.1",
    "rollup": "^0.54.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "sinon": "~4.1.2",
    "symbol-observable": "^1.1.0",
    "webpack": "~3.10.0"
  },
  "dependencies": {
    "axios": "~0.17.1",
    "cefc-utils": "~0.0.12",
    "eventemitter3": "~3.0.0",
    "qs": "~6.5.1",
    "redux": "~3.7.2",
    "redux-thunk": "~2.2.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "Android >= 4",
              "iOS >= 7",
              "Safari >= 7",
              "not ie <= 8"
            ]
          },
          "modules": false,
          "loose": true
        }
      ],
      "stage-0"
    ],
    "env": {
      "commonjs": {
        "presets": [
          [
            "env",
            {
              "loose": true
            }
          ]
        ]
      }
    }
  },
  "npmName": "svger-biz-utils",
  "npmFileMap": [
    {
      "basePath": "/lib/",
      "files": [
        "*.js"
      ]
    }
  ]
}
